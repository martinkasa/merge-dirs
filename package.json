{
  "name": "stratos-merge-dirs",
  "version": "0.2.3",
  "description": "node module to synchronously and recursively merge files from one folder to another",
  "main": "dist/index.js",
  "bin": "dist/bin.js",
  "scripts": {
    "clean": "rimraf test/c",
    "prepare": "babel src --out-dir dist",
    "mocha": "mocha --reporter spec --timeout 300 --require should --growl --require babel-core/register test/test.js",
    "test": "npm run clean && npm run mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/irfanhabib/merge-dirs.git"
  },
  "keywords": [
    "merge",
    "folder",
    "directory",
    "recursive"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "author": "Irfan Habib",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/irfanhabib/merge-dirs/issues"
  },
  "dependencies": {
    "inquirer": "^6.3.1",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "mocha": "~6.1.4",
    "rimraf": "^2.6.3",
    "should": "~13.2.3"
  },
  "resolutions": {
    "chokidar": "^2.0.4"
  }
}